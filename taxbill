#include <stdio.h>

//declare taxbill function, s=subtotal, C, T, N=salestax, t=taxedbill
float addtax (float s, float C, float T, float N)
{
  C = s * 1.075;
  T = s * 1.065;
  N = s * 1.08875;
  return C;
  return T;
  return N;
}

int main(void) 
{
   float s = 123;
   float C;
   float N;
   float T;
   C = addtax(s, C, T, N);
   T = addtax(s, C, T, N);
   N = addtax(s, C, T, N);
   printf("California taxed bill is $%f\n", C);
   printf("New York City taxed bill is $%f\n", N);
  printf("Texas taxed bill is $%f\n", T);
}
